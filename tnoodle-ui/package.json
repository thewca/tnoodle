{
    "name": "tnoodle-ui",
    "version": "0.1.0",
    "private": true,
    "homepage": "http://localhost:2014/scramble",
    "dependencies": {
        "@cubing/icons": "^1.1.2",
        "@reduxjs/toolkit": "^2.0.1",
        "axios": "^1.6.2",
        "bootstrap": "^4.6.0",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-bootstrap": "^2.9.1",
        "react-dom": "^18.2.0",
        "react-icons": "^4.12.0",
        "react-redux": "^9.0.4",
        "react-scripts": "5.0.1",
        "web-vitals": "^3.3.2",
        "react-inlinesvg": "^3.0.2",
        "react-color": "^2.19.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=false --coverage",
        "eject": "react-scripts eject",
        "prettier": "prettier --check .",
        "lint": "prettier --write ."
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/index.tsx",
            "!src/reportWebVitals.ts",
            "!src/react-app-env.d.ts",
            "!src/main/api/**.ts",
            "!src/main/model/**.ts",
            "!**/Interceptor.tsx"
        ],
        "moduleNameMapper": {
            "^axios$": "axios/dist/axios.js"
        }
    },
    "devDependencies": {
        "@babel/plugin-transform-private-property-in-object": "^7.23.4",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.10",
        "@types/lodash": "^4.14.195",
        "@types/node": "^20.6.2",
        "@types/react": "^18.2.46",
        "@types/react-dom": "^18.2.17",
        "@types/react-color": "^3.0.6",
        "prettier": "^2.8.8",
        "typescript": "^4.9.4"
    }
}
